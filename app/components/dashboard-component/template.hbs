{{yield}}

<div class="container-fluid">

  <h1 class="col-md-12 text-center">Front End MVC: A Comparison</h1>

  <div class="repos row">
  {{#each repos as |repo|}}
    <div class="panel panel-default col-md-4 text-center">
      <div class="panel-heading">
        <img src="{{repo.imagePath}}" alt="">
        <h3>{{repo.repoName}}</h3>
      </div>
      <div class="panel-body">
        <a href="{{repo.homepage}}" target="_blank">{{repo.description}}</a>
      </div>
      <div>
        <iframe scrolling="no" style="border:none;" width="440" height="430"
          src="http://www.google.com/trends/fetchComponent?hl=en-US&q={{repo.shortName}}+js&geo=US&tz=Etc/GMT%2B5&content=1&cid=GEO_MAP_0_0&export=5&w=440&h=330" frameborder="0">
        </iframe>
      </div>
    </div>
  {{/each}}
  </div>

  <div class="well well-lg col-md-12 text-center">
    <iframe scrolling="no" style="border:none;" width="900" height="330"
    src="http://www.google.com/trends/fetchComponent?hl=en-US&q=react+js,+angular+js,+ember+js&date=1/2012+48m&cmpt=q&tz=Etc/GMT%2B5&tz=Etc/GMT%2B5&content=1&cid=TIMESERIES_GRAPH_AVERAGES_CHART&export=5&w=900&h=330" frameborder="0">
    </iframe>
  </div>

  <div class="well well-lg col-md-12">
    {{high-charts
      content=lineData
      chartOptions=lineOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=starData
      chartOptions=starOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=subscriberData
      chartOptions=subscriberOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=forkData
      chartOptions=forkOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=issueData
      chartOptions=issueOptions
    }}
  </div>


</div>
