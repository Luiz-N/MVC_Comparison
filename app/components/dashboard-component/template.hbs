{{yield}}
<div class="container-fluid">

  <h1 class="col-md-12 text-center">Front End MVC: A Comparison</h1>

  <div class="repos row">
  {{#each repos as |repo|}}
    <div class="panel panel-default col-md-4 text-center">
      <div class="panel-heading">
        <img src="{{repo.imagePath}}" alt="">
        <h3>{{repo.repoName}}</h3>
      </div>
      <div class="panel-body">
        <a href="{{repo.homepage}}" target="_blank">{{repo.description}}</a>
      </div>
      <div>
        <iframe scrolling="no" style="border:none;" width="440" height="430"
          src="https://www.google.com/trends/fetchComponent?hl=en-US&q={{repo.shortName}}+js&date=today%2060-m&geo=US&tz=Etc/GMT%2B5&content=1&cid=GEO_MAP_0_0&export=5&w=440&h=330" frameborder="0">
        </iframe>
      </div>
    </div>
  {{/each}}
  </div>

  <div class="well well-lg col-md-10 col-md-offset-1 text-center">
    <iframe scrolling="no" style="border:none; background-color:white;" width="1050" height="350" background="white"
    src="https://www.google.com/trends/fetchComponent?hl=en-US&q=react+js,+angular+js,+ember+js&date=today%2060-m&cmpt=q&tz=Etc/GMT%2B5&tz=Etc/GMT%2B5&content=1&cid=TIMESERIES_GRAPH_AVERAGES_CHART&export=5&w=1200&h=350" frameborder="0">
    </iframe>
  </div>

  <div class="well well-lg col-md-12">
    {{high-charts
      content=lineData
      chartOptions=lineOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=starData
      chartOptions=starOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=subscriberData
      chartOptions=subscriberOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=forkData
      chartOptions=forkOptions
    }}
  </div>

  <div class="well well-lg col-md-6">
    {{high-charts
      content=issueData
      chartOptions=issueOptions
    }}
  </div>

  <div class="col-md-offset-5 col-md-2 text-center text-center row">
    A <a href="https://twitter.com/Hey_Luiz">@Hey_Luiz</a> Production
  </div>
</div>
